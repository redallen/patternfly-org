{
  "name": "@patternfly/org-patternfly-4",
  "description": "Documentation for Everything PatternFly at https://patternfly.org",
  "version": "1.0.0",
  "author": "Red Hat",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "build:deps": "yarn build:react && yarn build:next",
    "build:react": "cd deps/patternfly-react && yarn build:pf4",
    "build:next": "cd deps/patternfly-next && yarn build",
    "clean": "rimraf .cache public",
    "develop": "yarn clean && gatsby develop",
    "uninstall": "find . -name node_modules | xargs rm -rf"
  },
  "dependencies": {
    "@patternfly/patternfly": "^1.0.250",
    "gatsby": "^2.3.10",
    "gatsby-mdx": "^0.4.4",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-manifest": "^2.0.26",
    "gatsby-plugin-no-sourcemaps": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-sitemap": "^2.0.11",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-prismjs": "^3.2.6",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "node-sass-glob-importer": "^5.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/patternfly/patternfly-org"
  },
  "workspaces": {
    "packages": [
      "deps/patternfly-next",
      "deps/patternfly-react",
      "deps/patternfly-react/packages/react-icons",
      "deps/patternfly-react/packages/patternfly-4/*"
    ]
  }
}
